{
    "name": "flutter-code-select",
    "displayName": "flutter code select",
    "description": "Quick select code between brackets, support for (),{} and [],quotes, <>,\nif Is *Widget()*,the widget name will be included",
    "version": "1.1.1",
    "publisher": "wangxinhai-ref-chunsen",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.15.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tearf001/vscode-flutter-code-select.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:flutter-code-select.select",
        "onCommand:flutter-code-select.select-include",
        "onCommand:flutter-code-select.widget-include"
    ],
    "main": "./out/src/flutterCodeSelectMain",
    "contributes": {
        "commands": [
            {
                "command": "flutter-code-select.select",
                "title": "FluSel:Select"
            },
            {
                "command": "flutter-code-select.select-include",
                "title": "FluSel:SelectInclude"
            },
            {
                "command": "flutter-code-select.widget-include",
                "title": "FluSel:WidgetInclude"
            }
        ],
        "keybindings": [
            {
                "command": "flutter-code-select.select",
                "key": "alt+a",
                "mac": "alt+a",
                "when": "editorTextFocus"
            },
            {
                "command": "flutter-code-select.select-include",
                "key": "ctrl+alt+a",
                "mac": "cmd+alt+a",
                "when": "editorTextFocus"
            },
            {
                "command": "flutter-code-select.widget-include",
                "key": "ctrl+shift+w",
                "mac": "ctrl+shift+w",
                "when": "editorTextFocus"
            }
        ],
		"configuration": {
			"title": "Bracket Select configuration",
			"properties": {
				"flutter-code-select.openBracket": {
					"type": "array",
					"default": ["(", "{", "["],
					"description": "Characters to be recognized as opening brackets. MUST have a pair in openingBrackets."
				},
				"flutter-code-select.closeBracket": {
					"type": "array",
					"default": [")", "}", "]"],
					"description": "Characters to be recognized as closing brackets. MUST have a pair in closingBrackets."
				},
				"flutter-code-select.sameBracket": {
					"type": "array",
					"default": ["\"", "'", "`"],
					"description": "double quote is escaped for JSON"
				}
			}
		}
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^8.0.1",
        "typescript": "^2.0.3",
        "vscode": "^1.1.34"
    },
    "dependencies": {}
}
